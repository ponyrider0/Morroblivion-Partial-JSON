            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "I met an Argonian on the road near Bal Ur named Tul. He has no armor and no weapons I can see, and he desires my help.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
                            "VariableCount": 0, 
                            "Type": 1
