            "PNAM - Previous INFO": 0, 
            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "76 61 79", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "How interesting. Did you know that corprus makes you immune to disease? Have you ever heard of the prophecies of the Nerevarine?", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "76 61 79", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Ashlanders say the Nerevarine will be immune to disease.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "76 61 79", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "I've always thought, \"Maybe I have the Nerevarine down in my Corprusarium, and I don't even know it.\" Hah. Hah.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "76 61 79", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "The Nerevarine is a fat, disgusting corprus monster, and mad as a marsh rat. Wouldn't that be funny?", 
                    "NAM2 - Actor notes": ""
                }
            ], 
            "Conditions": [
                {
                    "Type": 96, 
                    "Unused": 0, 
                    "Comparison Value": 49.9999998737621, 
                    "Function": 58, 
                    "Parameter #1": "fbmwMQ07", 
