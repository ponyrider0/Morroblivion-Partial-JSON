                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Speak to Nevosi Hlan, shipmaster. His boat is the 'Chun-Ook'.", 
                    "NAM2 - Actor notes": ""
                }
