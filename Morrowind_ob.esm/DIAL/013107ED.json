                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "69 75 73", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Perhaps at Holamayan we should record and study dreams. We have so much to learn.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
