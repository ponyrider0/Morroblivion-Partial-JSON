                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "Please accept my apologies for not seeking you out and accepting you at once.", 
                    "NAM2 - Actor notes": ""
