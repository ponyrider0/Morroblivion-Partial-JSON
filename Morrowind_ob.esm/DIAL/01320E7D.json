                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "I have spoken with Athyn Sarethi. He believes your story and says you can be trusted.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "I have known Athyn Sarethi since he was a child, and I have absolute faith his judgement.", 
                    "NAM2 - Actor notes": ""
                }, 
