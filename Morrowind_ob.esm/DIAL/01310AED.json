            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "57 4D 51", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "I don't know much about the Ashlanders. Most people say they are murderous savages. But most people are idiots.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "74 61 64", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "I know they hate their settled Dunmer cousins almost as much as they hate Westerners. They must be tough to live in the Ashlands.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "74 61 64", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "I don't think outlanders can become members of the tribes. I don't know why anyone would want to.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
            "Conditions": [
                {
                    "Type": 96, 
                    "Unused": 0, 
                    "Comparison Value": 13.9999999646534, 
                    "Function": 58, 
                    "Parameter #1": "fbmwMQ01", 
