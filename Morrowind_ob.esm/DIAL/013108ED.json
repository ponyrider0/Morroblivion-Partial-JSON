                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "68 69 6D", 
                        "Response number": 5
                    }, 
                    "NAM1 - Response Text": "I would prefer that Arethan Mandas be cured of his madness, rather than killed.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "68 69 6D", 
                        "Response number": 6
                    }, 
                    "NAM1 - Response Text": "You might want to go to Ebonheart and speak with Llerar Mandas, Arethan's father.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
