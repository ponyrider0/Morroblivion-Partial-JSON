                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "These creatures appear to die, but always are revived at the Heart.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 09 3A", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "Somehow Dagoth Ur has conferred some portion of his immortality upon them.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 09 3A", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "Or perhaps they sustain themselves through more conventional sorcery.", 
                    "NAM2 - Actor notes": ""
                }
