                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "He brought us back from certain defeat. He moved the council here to Vvardenfell and took our share of the frontier lands.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "He is a natural leader, born to rule. One only wishes he was just and fair as well as strong.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
