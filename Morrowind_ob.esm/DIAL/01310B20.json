            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "The Temple teaches that the Three Good Daedra -- the Anticipations, Boethiah, Azura, and Mephala -- recognized the Divinity of the Tribunal and made homage to them.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "But the Rebel Daedra -- the Dark Daedra, Molag Bal, Malacath, Sheogorath, and Mehrunes Dagon -- in their arrogance and pride, refused to keep faith with the Tribunal.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "These Rebel Daedra thus became the Four Corners of the House of Troubles, and they continue to tempt the unwary into heresy and dark worship.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
            "Conditions": [
                {
                    "Type": 0, 
                    "Unused": 0, 
