            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "The Nords of Skyrim are a tall and fair-haired people, aggressive and fearless in war, industrious and enterprising in trade and exploration.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Skilled sailors, they can be found in seaports and settlements along all the coasts and rivers of Tamriel.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "Strong, stubborn, and hardy, they are famous for their resistance to cold, even magical frost.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "Violence is an accepted and comfortable aspect of Nord culture, and they cheerfully face battle with an ecstatic ferocity that shocks and appalls their foes.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
