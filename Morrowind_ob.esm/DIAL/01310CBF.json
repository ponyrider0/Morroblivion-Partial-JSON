                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Guild members never harm or steal from anyone - except the target of a legal Writ of execution. Never betray our secrets or our identities.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "If you break any of these rules, you must immediately speak with a Master to regain your good standing or you may be targeted for execution.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
