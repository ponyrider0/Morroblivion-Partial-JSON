            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "I was an ashkhan. I led my people against the Akaviri while the Cyrodiil dogs fled before them like kagouti.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "I quested deep into the strongholds and Red Mountain, cleansing the unclean things in their own blood.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "I did not heed the counsel of the women, and, to my shame, I craved glory, but never saw my real enemies.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "20 66 61", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "My axe and boots are yours, with my blessing.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
            "Conditions": [
                {
                    "Type": 0, 
                    "Unused": 0, 
                    "Comparison Value": 0.999999997475243, 
                    "Function": 72, 
