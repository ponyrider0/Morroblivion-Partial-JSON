                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "The Orc peoples of the Wrothgarian and Dragontail Mountains are brave and hardy barbarians.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Once they were feared and hated by everyone else in Tamriel. Now many have gained an education and Imperial citizenship through service in the legions.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "Their armorers are the finest in the world, and Orc warriors in heavy armor are the best front-line troops in all Tamriel.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "Detractors say that Orcs are rough and cruel, but Orcs say they are hard, but fair, and stern, but just.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
