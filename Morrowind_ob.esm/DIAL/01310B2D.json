            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "Molag Amur is the rocky, uninhabited wilderness interior of southeastern Vvardenfell.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "Molag Mar is the only settlement, and it's more a fortress than a village.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "Pilgrims come here to visit Mount Kand and Mount Assarnibibi, but they can't travel without well-equipped, heavily armed escorts.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "There are no paths, and ravines, ridges, and lava pools make it impossible to travel straight over land.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 5
                    }, 
                    "NAM1 - Response Text": "And if you get in trouble, you're on your own.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
            "Conditions": [
                {
                    "Type": 0, 
                    "Unused": 0, 
                    "Comparison Value": 0.999999997475243, 
                    "Function": 67, 
