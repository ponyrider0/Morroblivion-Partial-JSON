            }, 
            "QSTI - Quest": "MorroDefaultQuest", 
            "PNAM - Previous INFO": 0, 
            "Add topics": [
                "1Ashlanders", 
                "1foreigners", 
                "1FalseSGods", 
                "1Morrowind", 
                "1Ashlanders", 
                "1Ashlanders", 
                "1law", 
                "1theSEmpire", 
                "1Ashlanders"
            ], 
            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "Most Ashlanders wish all foreigners and their false gods could be driven from Morrowind.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "At very least, Ashlanders wish the foreign devils would leave them in peace.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "Ashlanders think it shameful to attack unarmed persons, but they will kill without hesitation an armed person who offends them or their clan laws.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "No Ashlander is fool enough to make war against the Empire.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 5
                    }, 
                    "NAM1 - Response Text": "However, if such a war might be won, many Ashlanders might cheerfully give their lives to win such a war.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
