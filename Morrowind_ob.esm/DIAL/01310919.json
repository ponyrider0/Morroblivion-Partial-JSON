            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "The Ascadian Isles is the green garden of Vvardenfell, a land of lakes and rivers, little farms and grand plantations.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "It's the lowland area just north of the city of Vivec.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "To the west lies Pelagiad and Balmora, to the north the ashlands of Bal Ur, and to the east the stinking smokes and lava pools of Molag Amur.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
            "Conditions": [
                {
                    "Type": 0, 
                    "Unused": 0, 
