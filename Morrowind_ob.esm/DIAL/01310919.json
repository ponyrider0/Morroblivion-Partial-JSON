            "Responses": [
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 1
                    }, 
                    "NAM1 - Response Text": "The Ascadian Isles is the lush, green, well-watered lowlands in the south where most of Vvardenfell's agriculture is found.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 2
                    }, 
                    "NAM1 - Response Text": "The area includes Pelagiad, Suran, Vivec City, and Ald Sotha along with the inland lakes and waterways of the Ascadian Isles proper.", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 3
                    }, 
                    "NAM1 - Response Text": "The urban areas of Vivec and Ebonheart of the southern coast are densely populated;", 
                    "NAM2 - Actor notes": ""
                }, 
                {
                    "TRDT - Response Data": {
                        "Emotion Type": 0, 
                        "Emotion Value": 50, 
                        "Unused": "CD CD CD", 
                        "Response number": 4
                    }, 
                    "NAM1 - Response Text": "the inland Ascadian Isles are dotted with small farms and large plantations. The climate is temperate and comfortable, with moderate rainfall.", 
                    "NAM2 - Actor notes": ""
                }
            ], 
